{
  "name": "page-qr",
  "version": "0.1.1",
  "description": "Chromium based extension to generate a QR code for your active browser tab",
  "keywords": [
    "chrome-extension",
    "qr"
  ],
  "author": "Michael Coakley <mcoakley@acmeframework.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/acmeframework/page-qr"
  },
  "homepage": "https://github.com/acmeframework/page-qr#README.md",
  "bugs": {
    "url": "https://github.com/acmeframework/page-qr/issues"
  },
  "scripts": {
    "build": "npm run clean:build && grunt build",
    "clean:build": "npx rimraf ./build/",
    "clean:all": "npm run clean:build",
    "fix": "npx eslint --fix '**/*.js'",
    "format": "npx prettier --write .",
    "lint": "npx eslint '**/*.js'"
  },
  "dependencies": {
    "qrcode": "^1.4.4"
  },
  "devDependencies": {
    "archiver": "^5.2.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "grunt": "^1.3.0",
    "grunt-contrib-copy": "^1.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2"
  }
}
